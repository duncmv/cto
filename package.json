  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.12",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vercel": "^34.2.7"
  },
  "engines": {
    "node": ">=18"

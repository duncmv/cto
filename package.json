{
  "name": "uganda-map-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "build:schools": "node scripts/build-schools.js",
